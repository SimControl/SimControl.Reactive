  <PropertyGroup>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Authors />
    <Company>SimControl e.U. - Wilhelm Medetz</Company>
    <Product>SimControl.Reactive</Product>
