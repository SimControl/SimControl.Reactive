    <PackageIcon>..\PackageIcon.png</PackageIcon>
    <RepositoryUrl>https://github.com/SimControl/SimControl.Reactive</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Version>0.8.0-prerelease.0.alpha</Version>
  </PropertyGroup>

</Project>
